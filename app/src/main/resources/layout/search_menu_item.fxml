<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox spacing="5" styleClass="custom-menu-item" xmlns="http://javafx.com/javafx/10.0.2-internal"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:id="searchMenuItemRoot">
    <Label fx:id="searchScopeLabel" textFill="black"/>
    <TextField fx:id="searchField" promptText="Type here..."/>
    <HBox alignment="BASELINE_CENTER" spacing="2">
        <Button fx:id="findPreviousButton" prefWidth="100" text="Prev"/>
        <Button fx:id="findNextButton" prefWidth="100" text="Next"/>
    </HBox>
</VBox>
